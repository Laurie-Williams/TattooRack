<!DOCTYPE html>
<html>
<head>
  <title>TattooRackA</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div id="nav">
  <%= link_to "Sign Up", new_user_registration_path unless current_user %>
  <%= link_to "Sign In", new_user_session_path unless current_user%>
  <%= link_to "Sign Out", destroy_user_session_path, method: :delete if current_user%>
  <%= link_to "Account Settings", edit_user_path(current_user) if current_user%>
</div>

<div id="Flash">
  <% flash.each do |type, message| %>
    <p class=<%= type %>><%= flash[type] %></p>
  <% end %>
</div>

<div id="main">
  <%= yield %>
</div>

</body>
</html>
